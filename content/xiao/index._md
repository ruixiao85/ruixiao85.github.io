from borax.calendars.lunardate import LunarDate
today=LunarDate.today()
today
today.animal
today.year
today.month
today.day
print(today.strftime('%Y年%L%M月%D'))
print(today.strftime('今天的干支表示法为：%G'))

LunarDate.from_solar_date(2020,10,1)

def print_both(dt):
  print(dt.to_solar_date().strftime('%Y-%m-%d'), end='')
  print("\\n", end='')
  print(dt.strftime('%o年%L%M月%D'), end='')
  # print(dt.strftime('%Y年%L%M月%D'))
  print()
  # print(dt.strftime('%G'))

def lunar(y,m,d, name=None):
  if name: print(name, end='\\n')
  print_both(LunarDate(y, m, d))

def solar(y,m,d, name=None):
  if name: print(name, end='\\n')
  print_both(LunarDate.from_solar_date(y, m, d))


